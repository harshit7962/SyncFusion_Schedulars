<Window x:Class="SyncFusion_Schedulars.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SyncFusion_Schedulars"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ThemeMode="Light">
    <StackPanel>
        <Button Margin="8" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource AccentButtonStyle}" Content="Open a Schedular Window" Click="Button_OpenSchedularClick" />
        <Button Margin="8" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource AccentButtonStyle}" Content="Open Time Selector Window" Click="Button_OpenTimeSelectorClick" />
    </StackPanel>
</Window>
